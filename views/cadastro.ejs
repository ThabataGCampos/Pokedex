<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
    <title>Register</title>
</head>
<body>
    <%- include('./partials/header.ejs') %>  
    <main id="mainCadastro">
        <fieldset class="cadastro">
            <h2 id="tituloCadastro">Register a new pokemon:</h2>

            <form action="/cadastro" method="post">
               
                    <fieldset>
                        <label for="number">Number</label>
                        <input type="text" id="number" name="number">
                    </fieldset>

                    <fieldset>
                        <label for="name">Name</label>
                        <input type="text" id="name" name="name" required>
                    </fieldset>

                    <fieldset>
                        <label for="type">Type</label>
                        <input type="text" id=type name=type required>
                    </fieldset>

                    <fieldset>
                        <label for="image">Image</label>
                        <input type="url" id="image" name="image">
                    </fieldset>

                    <fieldset>
                        <label for="description">Description</label>
                        <input type="text" id="description" name="description">
                    </fieldset>

                    <fieldset>
                        <label for="height">Height</label>
                        <input type="text" id="height" name="height">
                    </fieldset>

                    <fieldset>
                        <label for="weigth">Weigth</label>
                        <input type="text" id="weigth" name="weigth">
                    </fieldset>

                    <fieldset>
                        <label for="category">Category</label>
                        <input type="text" id="category" name="category">
                    </fieldset>

                    <fieldset>
                        <label for="abilities">Abilities</label>
                        <input type="text" id="abilities" name="abilities">
                    </fieldset>

                    <button id="buttonCad" type="submit">Send</button>
                
            </form>
        </fieldset>
    </main>
    
    <%-include('./partials/footer.ejs')%> 

</body>
</html>