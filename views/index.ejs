<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
    <title> Pokedex </title>
</head>

<body>
   <%- include('./partials/header.ejs') %> 
   
    <main>
        <section class="cards">
            <% pokedex.forEach(element => { %>
                <a href="detalhes/<%=element.id%>">
                <div class="card">
                    <div class="nomeDoPokemon"> <h3> <%=element.name%></h3> </div>
                    <div class="imagemDoPokemon"> <img src="<%=element.image%>" alt="<%=element.name%> picture"> </div>
                    <div class="tipoDoPokemon"> <p> <%=element.type%></p> </div>
                </div>
                </a>
            <% }) %>
        </section>
    </main>

   
 <%- include('./partials/footer.ejs') %> 
    
    
</body>
</html>
